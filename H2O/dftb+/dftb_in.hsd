Geometry = GenFormat {
 <<< xxx.hsd
}

Driver = {}

Hamiltonian = DFTB {
  SCC = Yes
  Eigensolver = ELPA {}
  MaxAngularMomentum = {
    Si = "p"
  }
  Filling = Fermi {
    Temperature [Kelvin] = 100.0
  }
  SlaterKosterFiles = {
    H-H = "H-H.skf"
    H-O = "H-O.skf"
    O-H = "O-H.skf"
    O-O = "O-O.skf"
  }
  KPointsAndWeights = {
    0.0 0.0 0.0 1.0
  }
  SCCTolerance = 1.000000000000000E-005
  Charge = 0.000000000000000E+000
  MaxSCCIterations = 5
}
Analysis = {
  CalculateForces = No
  ProjectStates = {}
  MullikenAnalysis = No
  AtomResolvedEnergies = No
  WriteEigenvectors = No
  WriteBandOut = No
}
Options = {
  WriteAutotestTag = No
  WriteDetailedOut = Yes
  WriteDetailedXML = No
  WriteResultsTag = No
  RestartFrequency = 20
  RandomSeed = 0
  WriteHS = No
  WriteRealHS = No
  MinimiseMemoryUsage = No
  ShowFoldedCoords = No
  TimingVerbosity = 0
  WriteChargesAsText = No
}
ParserOptions = {
  ParserVersion = 6
  WriteHSDInput = No
  WriteXMLInput = No
  StopAfterParsing = No
  IgnoreUnprocessedNodes = No
}
Parallel = {
  UseOmpThreads = No
  Groups = 1
  Blacs = BlockSize {
    16
  }
}
ExcitedState = {}
